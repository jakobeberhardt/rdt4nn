# RDT4NN Driver Configuration Template
# Copy this file to .env and fill in your actual values

# InfluxDB Configuration
INFLUXDB_HOST=https://your-influxdb-host.com
INFLUXDB_USER=your_username
INFLUXDB_TOKEN=your_admin_token_here
INFLUXDB_ORG=your_organization
INFLUXDB_BUCKET=your_bucket_name

# Docker Registry (optional, leave empty if using public registries)
DOCKER_REGISTRY_HOST=your-registry.com
DOCKER_REGISTRY_USERNAME=your_docker_username
DOCKER_REGISTRY_PASSWORD=your_docker_password
